{
    "manifest_version": 3,
    "name": "TikTok Unfollower",
    "version": "1.0",
    "description": "Automatically unfollow all TikTok users (Following + Friends).",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "host_permissions": ["https://www.tiktok.com/*"]
  }
  